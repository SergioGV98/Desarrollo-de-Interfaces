<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/invoice_detail_tvCliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tieCliente"
        android:textStyle="bold"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toTopOf="@+id/invoice_detail_tilCliente"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.084"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.634" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/invoice_detail_tilCliente"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="60dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/invoice_detail_tieCliente"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fontFamily="@font/helvetica"/>
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/invoice_detail_tilFechaEm"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilCliente">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/invoice_detail_tieFechaEm"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/helvetica"/>
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/invoice_detail_tilFechaFin"
        android:layout_width="350dp"
        android:layout_height="60dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilFechaEm">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/invoice_detail_tieFechaFin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/helvetica"/>
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/invoice_detail_tvFechaEm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tieFechaEm"
        android:textStyle="bold"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toTopOf="@+id/invoice_detail_tilFechaEm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.102"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilCliente"
        app:layout_constraintVertical_bias="0.516" />

    <TextView
        android:id="@+id/invoice_detail_tvFechaFin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tieFechaFin"
        android:textStyle="bold"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toTopOf="@+id/invoice_detail_tilFechaFin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilFechaEm"
        app:layout_constraintVertical_bias="0.516" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/invoice_detail_rvArticulos"
        android:layout_width="350dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilFechaFin" />

    <TextView
        android:id="@+id/invoice_detail_tvArticulos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/tieArticulosFactura"
        android:textStyle="bold"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toTopOf="@+id/invoice_detail_rvArticulos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.114"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_tilFechaFin"
        app:layout_constraintVertical_bias="0.538" />

    <TextView
        android:id="@+id/invoice_detail_tvPrecio"
        android:layout_width="40dp"
        android:layout_height="17dp"
        android:text="@string/tvTotal"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.617"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_rvArticulos"
        app:layout_constraintVertical_bias="0.509" />

    <TextView
        android:id="@+id/invoice_detail_tvTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/helvetica"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.268"
        app:layout_constraintStart_toEndOf="@+id/invoice_detail_tvPrecio"
        app:layout_constraintTop_toBottomOf="@+id/invoice_detail_rvArticulos"
        app:layout_constraintVertical_bias="0.49" />

</androidx.constraintlayout.widget.ConstraintLayout>